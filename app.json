{
  "expo": {
    "name": "Marcar para prestadores de servi√ßo",
    "slug": "marcar-prestador",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E42528"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marcar.prestador"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E42528"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAOpdZ5yzReKCQobwk3DAnPzGT_yrSDf4Q"
        }
      },
      "package": "com.marcar.prestador"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9d45fcb8-d676-4b0d-9e3f-dea6c9d6815d"
      }
    }
  }
}
